<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coursera.capstore.impatient"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.coursera.capstore.impatient.LoginImpatientActivity"
            android:label="@string/login_layout_title" />
        <activity android:name="com.coursera.capstore.impatient.LogoImpatientActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.coursera.capstore.impatient.MenuImpatientActivity"
            android:label="@string/menu_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.CheckInImpatientActivity"
            android:label="@string/checkin_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.WaitingListImpatientActivtity"
            android:label="@string/waiting_list_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.ProfileImpatientActivity"
            android:label="@string/profile_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.DoctorImpatientActivity"
            android:label="@string/doctor_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.EmailImpatientActivity"
            android:label="@string/email_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.TreatmentImpatientActivity"
            android:label="@string/treatment_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.DatePatientActivity"
            android:label="@string/date_layout_title" />
        <activity
            android:name="com.coursera.capstore.impatient.SettingImpatientActivity"
            android:label="@string/setting_layout_title" />
    </application>

</manifest>